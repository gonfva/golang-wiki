{
   "kind": "Service",
   "apiVersion": "v1",
   "metadata": {
      "name": "wiki-service",
      "labels": {
         "name": "wiki-service"
      }
   },
   "spec":{
      "createExternalLoadBalancer": true,
      "ports": [
         {
           "port": 80,
           "targetPort": 8080,
           "protocol": "TCP"
         }
      ],
      "selector":{
         "name":"wiki"
      }
   }
}
